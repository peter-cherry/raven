# Builder.io
NEXT_PUBLIC_BUILDER_API_KEY=d7e7e3dd91284e2dbc17f9afe5047bd6

# Instantly.ai - Email Dispatch (V2 API with SuperSearch)
INSTANTLY_API_KEY=your_instantly_api_key
