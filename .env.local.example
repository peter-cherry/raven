# Google Maps
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=your_google_maps_key
GOOGLE_MAPS_SERVER_KEY=your_google_maps_server_key
NEXT_PUBLIC_HOMEPAGE_AERIAL_ADDRESS=optional_default_aerial_view_address

# Mapbox (optional fallback for geocoding)
